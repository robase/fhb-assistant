{
  "name": "fhb-assistant",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "db:reset": "npx tsx db/reset.ts",
    "db:migrate-generate": "npx drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate-apply": "npx tsx db/migrate.ts",
    "db:studio": "npx drizzle-kit studio",
    "seed-content": "npx tsx db/seedDocs/main.ts",
    "docker": "docker compose up -d"
  },
  "dependencies": {
    "@langchain/core": "^0.2.15",
